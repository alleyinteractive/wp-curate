{
  "name": "wp-curate",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "main": "wp-curate.php",
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "scripts": {
    "build": "alley-build build",
    "create-block": "npx @alleyinteractive/create-block@latest -n wp-curate",
    "precheck-types": "check-node-version --package",
    "check-types": "tsc",
    "lint:fix": "eslint --ext .jsx --ext .js --ext .ts --ext .tsx . --fix",
    "lint": "npm run check-types && eslint --ext .jsx --ext .js --ext .ts --ext .tsx .",
    "packages-update": "alley-build packages-update --dist-tag=wp-6.4",
    "postinstall": "rm -rf node_modules/.cache/babel-loader && rm -rf node_modules/.cache/webpack",
    "prebuild": "check-node-version --package",
    "predev": "check-node-version --package",
    "preinstall": "check-node-version --package",
    "prelint:fix": "check-node-version --package",
    "prelint": "check-node-version --package",
    "prestylelint:fix": "check-node-version --package",
    "prestylelint": "check-node-version --package",
    "pretest:watch": "check-node-version --package",
    "pretest": "check-node-version --package",
    "start": "alley-build start",
    "start:hot": "alley-build start --hot",
    "stylelint:fix": "stylelint --fix \"**/*.scss\"",
    "stylelint": "stylelint \"**/*.scss\" --allow-empty-input",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@alleyinteractive/block-editor-tools": "^0.10.0",
    "@types/jest": "^29.5.12",
    "@types/wordpress__block-editor": "^11.5.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@wordpress/api-fetch": "^6.39.13",
    "@wordpress/block-editor": "^12.10.13",
    "@wordpress/blocks": "^12.19.13",
    "@wordpress/components": "^25.8.13",
    "@wordpress/core-data": "^7.3.0",
    "@wordpress/data": "^9.12.13",
    "@wordpress/edit-post": "^7.19.15",
    "@wordpress/element": "^5.19.13",
    "@wordpress/i18n": "^4.42.13",
    "@wordpress/icons": "^10.3.0",
    "@wordpress/plugins": "^6.10.13",
    "@wordpress/url": "^3.43.13",
    "classnames": "^2.5.1",
    "dompurify": "^3.1.6",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@alleyinteractive/build-tool": "^0.1.4",
    "@alleyinteractive/eslint-config": "^0.1.6",
    "@alleyinteractive/stylelint-config": "^0.0.2",
    "@babel/preset-env": "^7.24.8",
    "@types/wordpress__edit-post": "^7.5.1",
    "@types/wordpress__plugins": "^3.0.0",
    "@wordpress/babel-preset-default": "^7.26.13",
    "babel-jest": "^29.5.0",
    "check-node-version": "^4.2.1",
    "jest": "^29.6.4",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3",
    "webpack-cli": "^5.1.4",
    "wp-types": "^3.65.0"
  }
}
