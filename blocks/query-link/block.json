{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wp-curate/query-link",
  "version": "0.1.0",
  "title": "Query Link",
  "category": "theme",
  "icon": "admin-links",
  "description": "Display \"See All\" link for a custom query.",
  "textdomain": "wp-curate",
  "editorScript": "file:index.ts",
  "usesContext": [
    "query"
  ],
  "render": "file:render.php",
  "attributes": {
    "seeAllText": {
      "type": "string",
      "default": "See All"
    },
    "urlOverride": {
      "type": "string"
    }
  },
  "parent": [
    "wp-curate/query",
    "core/post-template"
  ]
}
