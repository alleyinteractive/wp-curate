{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wp-curate/post-title",
	"version": "0.1.0",
	"title": "Post Title",
	"category": "theme",
	"icon": "post",
	"description": "Custom post title for WP Curate",
	"textdomain": "post-title",
	"editorScript": "file:index.ts",
  "style": "wp-block-post-title",
  "usesContext": ["postId", "query", "pinnedPosts"],
  "attributes": {
    "level": {
      "type": "number",
      "default": 3
    },
    "customPostTitles": {
      "default": [],
      "items": {
        "default": {},
        "type": "object"
      },
      "type": "array"
    }
  },
	"render": "file:render.php"
}
